<?xml version="1.0"?>
<automaton width="60" height="60">

	<zerostate name="." />
	<state name="a" />
	<state name="x" />

	<rule oldstate="a." newstate=".a" probability="0.8"/>
	<rule oldstate=".a" newstate="a." probability="0.8"/>
	<rule oldstate="ax" newstate="xx" probability="1"/>
	<rule oldstate="xa" newstate="xx" probability="1"/>

	<box x="0" y="0" width="60" height="60" probability="0.2" state="a" />

	<cell x="30" y="30" state="x" />
	<cell x="31" y="30" state="x" />
	<cell x="30" y="31" state="x" />
	<cell x="31" y="31" state="x" />

</automaton>

